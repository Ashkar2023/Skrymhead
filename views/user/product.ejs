					<script>
						let currentVariant = 0;
						let cartQuantity = document.querySelector("#cartQuantity");
						let stock = document.getElementById("stock");
						let quantity = document.getElementById("cartQuantity");
						let offer = Number("<%= greaterOffer %>")
						
						let qty = $("[name='cartQuantity']")
						$("#decrementBtn").css("cursor","no-drop");

						0 === Number(stock.dataset.stock) ? $("button#addToCart").prop("disabled",true).css({"filter":"grayscale(100%)","cursor":"no-drop"}) : $("button#addToCart").prop("disabled",false).css({"filter":"","cursor":"default"}) ;
						
						function updateQty(action) {
							if (action === "add" && qty.val() < Number(stock.dataset.stock) ) {
								qty.val(Number(qty.val()) + 1);
							} else if (action === "sub" && qty.val()>1) {
								qty.val(Number(qty.val()) - 1);
							}
							qty.val() === stock.dataset.stock ? $("#incrementBtn").css("cursor","no-drop") : $("#incrementBtn").css("cursor","default"); 
 
							$("#decrementBtn").css("cursor", Number(qty.val()) !== 1 ? "pointer" : "no-drop" )
						}

						function addToCart(product) {

							if (cartQuantity.value < 1) {
								Swal.fire({
									icon: 'warning',
									title: 'Invalid Quantity!',
									text: "Quantitiy can't be zero",
									timer: 4000,
									showConfirmButton: true
								});

								cartQuantity.value = "1";
								return;
							}

							fetch("/productaddtocart", {
								method: "PUT",
								headers: {
									"Content-Type": "application/json"
								},
								body: JSON.stringify({
									product: product,
									variant: currentVariant,
									quantity: cartQuantity.value
								})
							})
								.then(response => {
									if (response.ok) {
										return response.json();
									} else if (response.status === 409) {
										return response.json();
									}
								})
								.then(result => {
									if (result.success) {
										console.log(result.message);
										Swal.fire({
											icon: 'success',
											title: 'Success!',
											text: 'Cart updated successfully!',
											timer: 1000,
											showConfirmButton: false
										});
									} else {
										Swal.fire({
											icon: 'error',
											title: 'Conflict!',
											text: result.message,
											timer: 1000,
											showConfirmButton: true
										});
										console.log(result.message)
									}
								})
								.catch(error => {
									console.log(error.message)
								})
						}

						function selectVariant(index) {
							const currentUrl = window.location.href;
							const url = new URL(currentUrl);
							const id = url.searchParams.get("id");
							qty.val(1);

							fetch(`/product/variant/${index}?id=${id}`, {
								method: "GET",
								headers: {
									"Content-Type": "application/json",
								}
							})
								.then(response => {
									return response.json()
								})
								.then(data => {
									if (data.success) {
										variantUpdate(data.variantData, data.index)
									} else {
										console.log(data.message);
									}
								})
								.catch(error => {
									console.error('Error fetching variant:', error);
								});
						}

						function variantUpdate(variant, index) {
							let button = document.querySelector(".btn-lg");
							button.classList.remove("btn-lg");
							document.getElementById(`V${Number(index)}`).classList.add("btn-lg");
							currentVariant = index;
							console.log(currentVariant)
							

							let price = document.getElementById("price");
							let offerPrice= document.getElementById("offerPrice");
							calculatedAmount = variant.price - (offer/100) * variant.price;
							
							if(offer){
								offerPrice.innerHTML = calculatedAmount.toLocaleString("en-IN");
							}else{
								offerPrice.innerHTML = variant.price.toLocaleString("en-IN");
							}
							
							stock.textContent = variant.stock ? `${variant.stock} Units left` : "Out of stock";
							stock.dataset.stock = variant.stock;

							!variant.stock ? stock.classList.replace("text-dark", "text-danger") : stock.classList.replace("text-danger", "text-dark")
							stock.textContent==="Out of stock" ? $("#addToCart").prop("disabled",true).css({"filter":"grayscale(100%)","cursor":"no-drop"}): $("#addToCart").prop("disabled",false).css({"filter":"","cursor":"default"}) ;
						}
					</script>

